{
    "name": "mileschou/phalog",
    "description": "Static HTML generator",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "template"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "homepage": "https://mileschou.github.io/",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "erusev/parsedown": "^1.7",
        "illuminate/console": "^7",
        "illuminate/events": "^7",
        "illuminate/log": "^7",
        "illuminate/view": "^7",
        "laravel-bridge/scratch": "^0.7.0",
        "mikey179/vfsstream": "^1.6",
        "mileschou/codegener": "^1.2",
        "mileschou/parkdown": "^0.1",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.14",
        "phpunit/phpunit": "^8",
        "psy/psysh": "^0.9.12",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Phalog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "bin": [
        "phalog"
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
